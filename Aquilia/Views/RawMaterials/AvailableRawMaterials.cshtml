 @model IList<Aquilia.Models.RawMaterials>
<!DOCTYPE html>
<html>
<head runat="server">
    <title>Top Wholesellers</title>
</head>
<body>
    <table id="example" class="table table-striped table-bordered" style="width:97%; background-color:white; margin:20px;">
        <thead>
            <tr>
                <th>Vendor Name</th>
                <th>Item</th>
                <th>Rate</th>
                
                
            </tr>
        </thead>
        <tbody>
        <tfoot>
            @foreach (var rawmaterials in Model)
            {
            <tr>
                <td>@rawmaterials.VendorID</td>
                <td>@rawmaterials.RawMaterialID</td>

                <td>@rawmaterials.Rate</td>
                

            </tr>
}
        </tfoot>
    </table>
    <script>$(document).ready(function () {
            $('#example').DataTable();
        });
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.2/css/bootstrap.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

            <link rel="stylesheet" href="https://cdn.datatables.net/1.10.23/css/dataTables.bootstrap4.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous"></script>
</body>

</html>


