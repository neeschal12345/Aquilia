@model IList<Aquilia.Models.ProductParts>
<!DOCTYPE html>
<html>
<head runat="server">
    <title></title>
</head>
<body>
    <table id="example" class="table table-striped table-bordered" style="width:97%; background-color:white; margin:20px;">
        <thead>
            <tr>
                <th>Product Part ID</th>
                <th>Final ProductID</th>
                <th>Product Part Name</th>
                <th>Product Part Code</th>
                <th>Wax Composition</th>
                <th>Copper Composition</th>
                <th>Product Part Category</th>
                <th>Product Part Quantity</th>

            </tr>
        </thead>
        <tbody>

        <tfoot>
            @foreach (var productParts in Model)
            {
            <tr>
                <td>@productParts.ProductPartID</td>
                <td>@productParts.FinalProductID</td>
                <td>@productParts.ProductPartName</td>
                <td>@productParts.ProductPartNumber</td>
                <td>@productParts.WaxComposition</td>
                <td>@productParts.CopperComposition</td>
                <td>@productParts.ProductPartCategory</td>
                <td>@productParts.ProductPartQuantity</td>

            </tr>
            }
            
        </tfoot>
        </table>
        <script>$(document).ready(function () {
                $('#example').DataTable();
            });
            <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.5.2/css/bootstrap.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

                <link rel="stylesheet" href="https://cdn.datatables.net/1.10.23/css/dataTables.bootstrap4.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous"></script>
</body>

</html>


